{
  "extends": ["eslint:recommended", "airbnb"],
  "rules": {
    "no-underscore-dangle": 0,
    "comma-dangle": 0,
    "no-param-reassign": 0,
    "no-empty": 0,
    "no-else-return": 0,
    "no-unused-expressions": 0,
    "no-plusplus": 0,
    "no-nested-ternary": 0,
    "one-var": 0,
    "import/prefer-default-export": 0,
    "arrow-parens": 0,
    "arrow-body-style": 0,
    "object-property-newline": 0,
    "one-var-declaration-per-line": 0,
    "func-names": 0,
    "prefer-template": 0,
    "import/no-extraneous-dependencies": [0, {"devDependencies": ["test/*.spec.js"]}]
  }
}
